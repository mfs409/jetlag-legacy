{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "sourceMap": true,
    "outDir": "./check/",
    "noImplicitAny": true,
    "removeComments": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/game/*",
    "src/tutorials/assets_audio_animations.ts",
    "src/tutorials/collision.ts",
    "src/tutorials/endless_runner_game.ts",
    "src/tutorials/gestures_projectiles.ts",
    "src/tutorials/getting_started.ts",
    "src/tutorials/joints.ts",
    "src/tutorials/maze_game.ts",
    "src/tutorials/misc_starter_games.ts",
    "src/tutorials/movement_physics.ts",
    "src/tutorials/overhead_fight_farm_game.ts",
    "src/tutorials/overview.ts",
    "src/tutorials/package_desktop.ts",
    "src/tutorials/package_mobile.ts",
    "src/tutorials/platformer_game.ts",
    "src/tutorials/simulation_conway.ts",
    "src/tutorials/stage_transitions.ts",
    "src/tutorials/storage.ts",
    "src/tutorials/text_hud.ts",
    "src/tutorials/timers.ts",
    "src/viewer/*",
  ],
  "typedocOptions": {
    "out": "./docs/",
    "name": "JetLag Documentation",
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "hideGenerator": true
  }
}
